#!name=YouTube去广告(>=iOS15)
#!desc=去瀑布流广告/贴片广告

[Script]
去瀑布流广告 = type=http-response,script-path=https://raw.githubusercontent.com/Maasea/sgmodule/master/Script/Youtube/youtube.js,pattern=^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/(browse|next|player|search|reel\/reel_watch_sequence),max-size=3145728,requires-body=true,binary-mode=true,timeout=10,script-update-interval=0,enable=false
去贴片广告 = type=http-response,script-path=https://raw.githubusercontent.com/fhqievg/rules/main/script/youtube/removePlayAd.js,pattern=^https?:\/\/[\w-]+\.googlevideo\.com\/(?!(dclk_video_ads|videoplayback\?)).+&oad,max-size=1048576,timeout=10,script-update-interval=0,enable=true

[MITM]
hostname = %APPEND% *.googlevideo.com,youtubei.googleapis.com
